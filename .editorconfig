# http://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = true

[{**.js,**.php,**.xml}]
indent_size = 4

[{**.json,**.yml}]
indent_size = 2

# Markdown
[**.md]
indent_size = 2
trim_trailing_whitespace = false